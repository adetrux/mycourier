{
  "extends": [
    "tslint:recommended",
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "jsx-wrap-multiline": false,
    "object-literal-sort-keys": false,
    "no-debugger": false,
    "no-console": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
